  /* Login Modal Styles */
  .login-modal {
    display: none;
    position: fixed;
    inset: 0;
    background: #0B1C3F;
    color: white;
    z-index: 10000;
    padding: 2rem;
    overflow: auto;
  }

  .login-modal__content {
    max-width: 400px;
    margin: 0 auto;
    position: relative;
  }

  .login-modal__close {
    background: none;
    border: none;
    color: white;
    font-size: 24px;
    position: absolute;
    right: 0;
    top: 0;
    cursor: pointer;
  }

  .login-modal__title {
    font-size: 22px;
    font-weight: 600;
    margin-top: 2rem;
    margin-bottom: 0.5rem;
  }

  .login-modal__subtitle {
    margin-bottom: 1.5rem;
    color: rgba(255, 255, 255, 0.8);
  }

  .login-modal__input-label {
    display: block;
    margin-bottom: 8px;
  }

  .login-modal__input {
    width: 100%;
    padding: 10px;
    border-radius: 8px;
    border: none;
    margin-bottom: 1rem;
    background: white;
  }

  .login-modal__phone-group {
    display: flex;
    align-items: center;
    gap: 8px;
    margin-bottom: 1rem;
  }

  .login-modal__country-code {
    padding: 10px;
    border-radius: 8px;
    border: none;
    background: white;
  }

  .login-modal__submit {
    width: 100%;
    background-color: #444;
    color: white;
    padding: 12px;
    border-radius: 999px;
    font-size: 16px;
    border: none;
    cursor: pointer;
    margin-top: 1rem;
  }

  .login-modal__email-link {
    text-align: center;
    color: #750d0c;
    margin-top: 1rem;
    cursor: pointer;
    display: block;
  }

  .login-modal__footer {
    font-size: 12px;
    color: #ccc;
    margin-top: 2rem;
  }

  .login-modal__footer a {
    color: #750d0c;
    text-decoration: none;
  }

  /* Restaurant Section Styles */
  .restaurant-section {
    padding: 2rem 1rem;
    color: white;
    background: #0B1C3F;
  }

  .restaurant-section__content {
    max-width: 1200px;
    margin: 0 auto;
  }

  .restaurant-section__title {
    margin-bottom: 1rem;
    font-size: 18px;
    font-weight: 600;
  }

  .restaurant-carousel {
    display: flex;
    gap: 16px;
    overflow-x: auto;
    padding: 0 1rem 1rem 1rem;
    -webkit-overflow-scrolling: touch;
    scrollbar-width: none;
  }

  .restaurant-carousel::-webkit-scrollbar {
    display: none;
  }

  /* Restaurant Card Styles */
  .restaurant-card {
    min-width: 240px;
    background: #1f2a48;
    border-radius: 16px;
    overflow: hidden;
    cursor: pointer;
    transition: transform 0.2s;
    position: relative;
  }

  .restaurant-card:hover {
    transform: translateY(-2px);
  }

  .restaurant-card__image {
    width: 100%;
    height: 140px;
    object-fit: cover;
  }

  .restaurant-card__content {
    padding: 1rem;
  }

  .restaurant-card__title {
    margin: 0 0 6px;
    font-size: 16px;
    font-weight: 600;
  }

  .restaurant-card__meta {
    margin: 0;
    font-size: 14px;
    color: #ccc;
  }

  .fa-star {
    color: #FFD700;
    font-size: 12px;
    margin-right: 4px;
  }

  .restaurant-card__meta .fa-star {
    font-size: 12px;
    margin-right: 2px;
  }

  .restaurant-card__slots {
    display: flex;
    gap: 8px;
    flex-wrap: wrap;
    margin-top: 10px;
  }

  .time-slot {
    background-color: #750d0c;
    border: none;
    color: white;
    border-radius: 8px;
    padding: 6px 10px;
    cursor: pointer;
    font-size: 14px;
    transition: background-color 0.2s;
  }

  .time-slot:hover {
    background-color: #5a0a09;
  }

  /* Restaurant Detail Modal Styles */
  .restaurant-detail {
    display: none;
    position: fixed;
    inset: 0;
    background: #0B1C3F;
    color: white;
    overflow-y: auto;
    z-index: 10000;
  }

  .restaurant-detail__content {
    max-width: 768px;
    margin: auto;
    padding: 1rem;
  }

  .restaurant-detail__close {
    float: right;
    font-size: 24px;
    background: none;
    border: none;
    color: white;
    cursor: pointer;
  }

  .restaurant-detail__banner {
    width: 100%;
    height: 240px;
    object-fit: cover;
    border-radius: 16px;
    margin-bottom: 1rem;
  }

  .restaurant-detail__tabs {
    display: flex;
    gap: 1rem;
    margin: 1rem 0;
    border-bottom: 1px solid rgba(255, 255, 255, 0.1);
    padding-bottom: 1rem;
  }

  .tab-button {
    background: none;
    border: none;
    color: #ccc;
    font-size: 16px;
    cursor: pointer;
    padding: 0.5rem 1rem;
    border-radius: 999px;
  }

  .tab-button--active {
    color: white;
    background: rgba(255, 255, 255, 0.1);
  }

  .menu-items {
    display: flex;
    flex-direction: column;
    gap: 1rem;
  }

  .menu-item {
    display: flex;
    gap: 1rem;
    padding: 1rem;
    background: rgba(255, 255, 255, 0.05);
    border-radius: 12px;
  }

  .menu-item img {
    width: 80px;
    height: 80px;
    object-fit: cover;
    border-radius: 8px;
  }

  .menu-item__info h4 {
    margin: 0 0 0.5rem;
  }

  .menu-item__info p {
    margin: 0;
    font-size: 14px;
    color: #ccc;
  }

  .menu-item__meta {
    margin-top: 0.5rem;
    font-size: 14px;
    color: #999;
  }

  .menu-item__meta span {
    margin-right: 1rem;
  }

  .view-all-button {
    width: 100%;
    padding: 1rem;
    background: transparent;
    border: 1px solid rgba(255, 255, 255, 0.2);
    color: white;
    border-radius: 999px;
    margin: 1rem 0;
    cursor: pointer;
    transition: background-color 0.2s;
  }

  .view-all-button:hover {
    background: rgba(255, 255, 255, 0.1);
  }

  /* Trending List Styles */
  .trending-list {
    display: flex;
    flex-direction: column;
    gap: 12px;
    padding: 0 1rem;
  }

  .trending-item {
    display: flex;
    align-items: flex-start;
    gap: 16px;
    padding: 12px;
    background: rgba(255, 255, 255, 0.05);
    border-radius: 12px;
    transition: background-color 0.2s;
  }

  .trending-item:hover {
    background: rgba(255, 255, 255, 0.1);
  }

  .trending-item__image {
    width: 60px;
    height: 60px;
    border-radius: 50%;
    object-fit: cover;
  }

  .trending-item__content {
    flex: 1;
  }

  .trending-item__info {
    display: flex;
    align-items: center;
    gap: 12px;
    margin-top: 4px;
  }

  .trending-item__right {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 8px;
  }

  .trending-item__distance {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 4px;
    color: #e0e0e0;
    font-size: 14px;
    margin-top: 8px;
  }

  .trending-item__distance i {
    font-size: 16px;
  }

  .trending-item__price {
    color: #e0e0e0;
    font-size: 14px;
    text-align: center;
  }

  .trending-item__title {
    font-size: 16px;
    font-weight: 600;
    margin: 0 0 4px;
    color: white;
  }

  .trending-item__location {
    font-size: 14px;
    color: #ccc;
    margin: 0;
  }

  .trending-item__cuisine {
    font-size: 14px;
    color: #999;
    margin: 0;
  }

  .trending-item__rating {
    display: flex;
    align-items: center;
    gap: 4px;
    margin-top: 4px;
  }

  .trending-item__stars {
    color: #FFD700;
    font-size: 12px;
  }

  .trending-item__reviews {
    color: #ccc;
    font-size: 14px;
  }

  .trending-item__save {
    color: #ff0000;
    background: none;
    border: none;
    padding: 8px;
    cursor: pointer;
    transition: color 0.2s;
  }

  .trending-item__save:hover {
    color: #cc0000;
  }

  /* Mobile Menu Styles */
  .mobile-menu-toggle {
    display: none;
    background: none;
    border: none;
    color: white;
    font-size: 24px;
    cursor: pointer;
    padding: 8px;
  }

  /* Responsive Styles */
  @media screen and (max-width: 768px) {
    .header__content {
      gap: 0.5rem;
    }

    .header__greeting {
      font-size: 1.25rem;
    }

    .header__row {
      flex-direction: column;
      gap: 0.5rem;
    }

    .header__actions {
      width: 100%;
      justify-content: space-between;
      margin-left: 0;
    }

    .booking-buttons {
      width: 100%;
    }

    .booking-button {
      width: 100%;
      padding: 0.5rem 1rem;
    }

    /* Restaurant Cards */
    .restaurant-card {
      min-width: 200px;
    }

    .restaurant-card__image {
      height: 120px;
    }

    /* Trending List */
    .trending-item {
      padding: 8px;
      gap: 12px;
    }

    .trending-item__image {
      width: 50px;
      height: 50px;
    }

    .trending-item__title {
      font-size: 14px;
    }

    .trending-item__location,
    .trending-item__cuisine,
    .trending-item__reviews {
      font-size: 12px;
    }

    .trending-item__right {
      gap: 4px;
    }

    .trending-item__distance,
    .trending-item__price {
      font-size: 12px;
    }

    /* Modal Adjustments */
    .booking-modal {
      height: 100%;
      border-radius: 0;
    }

    .booking-modal__party-size {
      width: 2.5rem;
      height: 2.5rem;
    }

    .restaurant-detail__content {
      padding: 0.5rem;
    }

    .restaurant-detail__tabs {
      overflow-x: auto;
      padding-bottom: 0.5rem;
      gap: 0.5rem;
    }

    .tab-button {
      font-size: 14px;
      padding: 0.25rem 0.75rem;
      white-space: nowrap;
    }

    .menu-item {
      flex-direction: column;
    }

    .menu-item img {
      width: 100%;
      height: 160px;
    }

    .fa-star {
      font-size: 10px;
      margin-right: 2px;
    }

    .trending-item__stars {
      font-size: 10px;
    }

    .restaurant-card__meta .fa-star {
      font-size: 10px;
    }
  }

  @media screen and (max-width: 480px) {
    .header {
      padding: 0.5rem;
    }

    .restaurant-section {
      padding: 1rem 0.5rem;
    }

    .restaurant-carousel {
      padding: 0 0.5rem 0.5rem 0.5rem;
    }

    .trending-list {
      padding: 0 0.5rem;
    }

    .restaurant-card {
      min-width: 160px;
    }

    .restaurant-card__content {
      padding: 0.75rem;
    }

    .restaurant-card__title {
      font-size: 14px;
    }

    .restaurant-card__meta {
      font-size: 12px;
    }

    .time-slot {
      padding: 4px 8px;
      font-size: 12px;
    }

    .login-modal__content {
      padding: 1rem;
    }
  }

